@model NewCoreProject.Models.User

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="container-fluid" style="overflow:auto;">
<div>
        <h1 class="text-center user-name">Accounts</h1>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Admin Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Admin_Name)
        </dd>
        <dt class = "col-sm-2">
          Admin Email
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Admin_Email)
        </dd>
        <dt class = "col-sm-2">
           Admin Password
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Admin_Password)
        </dd>
        <dt class = "col-sm-2">
          Admin Contact
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Admin_Contact)
        </dd>
        <dt class = "col-sm-2">
           Admin Address
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Admin_Address)
        </dd>
        <dt class = "col-sm-2">
           Is Active
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IsActive)
        </dd>
      @*   <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ProfileImagePath)
        </dt> 
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ProfileImagePath)
        </dd> *@
        <dt class = "col-sm-2">
           Created At
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CreatedAt)
        </dd>
    </dl>
</div>
<div>
 <a asp-action="Index" class="btn btn-secondary">Back</a>
